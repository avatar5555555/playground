{
  "name": "playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "lint-staged",
    "postinstall": "if [ ! -f .env ]; then cp .env.example .env; fi",
    "dev": "nodemon ssr/index.js",
    "build": "next build && npm run lang",
    "lang:extract": "extract-messages -l=en,ru -o ./lang -d en --flat false 'src/**/*.{ts,tsx}'",
    "start": "NODE_ENV=production node ssr/index.js",
    "export": "next export",
    "analyze": "ANALYZE=1 next build",
    "schema:download": "sh -ac 'npx apollo schema:download --endpoint=$(grep 'GRAPHQL_API_URL=' .env | cut -d= -f2)'",
    "codegen": "apollo codegen:generate --addTypename --schema=./schema.json  --outputFlat --queries=\"src/**/*.{ts,tsx}\"  API.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@commitlint/cli": "^7.0.0",
    "@types/cookie": "^0.3.1",
    "@types/express": "^4.16.0",
    "@types/next": "^6.0.4",
    "@types/node": "^10.5.2",
    "@types/ramda": "^0.25.35",
    "@types/react": "^16.4.6",
    "@types/react-intl": "^2.3.8",
    "@types/yup": "^0.24.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-styled-components": "^1.5.1",
    "cz-customizable": "^5.2.0",
    "dotenv": "^6.0.0",
    "extract-react-intl-messages": "^0.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.0",
    "accepts": "^1.3.5",
    "apollo": "^1.5.0",
    "apollo-boost": "^0.1.10",
    "apollo-client": "^2.3.5",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "apollo-link-logger": "^1.2.2",
    "cookie": "^0.3.1",
    "express": "^4.16.3",
    "final-form": "^4.8.3",
    "flat": "^4.1.0",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "hoist-non-react-statics": "^3.0.0-rc.1",
    "intl": "^1.2.5",
    "next": "^6.1.1",
    "node-fetch": "^2.2.0",
    "nprogress": "^0.2.0",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-final-form": "^3.6.5",
    "react-intl": "^2.4.0",
    "smooth-ui": "^4.3.0",
    "styled-components": "^3.3.3",
    "styled-normalize": "^8.0.0",
    "yup": "^0.26.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
